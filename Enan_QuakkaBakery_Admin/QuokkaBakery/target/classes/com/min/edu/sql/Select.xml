<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.min.edu.model.SelectDaoImpl">

<resultMap type="acvo" id="acMap">
	<result column="AC_CODE" property="ac_code"/>
	<result column="AC_NAME" property="ac_name"/>
	<result column="AC_REGNUM" property="ac_regnum"/>
	<result column="REGDATE" property="regdate"/>
	<result column="AC_TEL" property="ac_tel"/>
	<result column="CEO_NAME" property="ceo_name"/>
</resultMap>
	
<!-- 비밀번호 찾기 -->
<select id="selectPW" parameterType="java.lang.String" resultType="java.lang.Integer">
	SELECT PW
			FROM ADMIN
				WHERE ID = #{id}
</select>

<!-- 아이디 찾기 select -->
<select id="selectID" parameterType="java.lang.String" resultType="java.lang.Integer">
	SELECT ID
			FROM ADMIN
				WHERE PW = #{x}
</select>

</mapper>
