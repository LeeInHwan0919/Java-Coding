<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.min.edu.model.DeleteDaoImpl">


<delete id="deleteAdmin" parameterType="java.util.Map">
DELETE FROM ADMIN
	WHERE ID = #{id}
</delete>

<delete id="deleteB" parameterType="java.util.Map">
DELETE FROM ACCOUNT
	WHERE AC_NAME = #{ac_name}
</delete>

<delete id="deleteC" parameterType="java.util.Map">
DELETE FROM ACCOUNT
	WHERE AC_TEL = #{ac_tel}
</delete>
</mapper>
